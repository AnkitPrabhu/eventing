namespace flatbuf.cfg;

table Config {
  appCode:string;
  appName:string;
  depCfg:DepCfg;
  functionID:uint;
  functionInstanceID:string;
  id:uint;
  usingTimer:bool;
  srcMutationEnabled:bool;
}

table DepCfg {
  buckets:[Bucket];
  metadataBucket:string;
  sourceBucket:string;
}

table Bucket {
  bucketName:string;
  alias:string;
  access:string;
}

root_type Config;
